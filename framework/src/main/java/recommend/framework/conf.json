{
  "functors":[
    {
      "name":"MainManager",
      "class":"Manager",
      "open":1,
      "管理":"管理算子，调度执行其它算子",
      "functors":"BeginManager,FeatureManager,RecallManager,FilterManager,SortManager,AdjustManager,ResortManager,PositionManager,EndManager",
      "config": {}
    },
    {
      "name":"BeginManager",
      "class":"Manager",
      "open":1,
      "开始":"串行执行一些准备工作，后边算子会依赖前边算子的输出",
      "functors":"GetToken,RequestToUserContext,ABExpParam",
      "config": {}
    },
    {
      "name":"FeatureManager",
      "class":"Manager",
      "open":1,
      "特征":"并行获取用户特征",
      "functors":"SexFeature,SessionFeature,SimpleFeature"
    },
    {
      "name":"RecallManager",
      "class":"Manager",
      "open":1,
      "召回":"并行召回",
      "functors":"SimpleRedis,PosRecall,SessionRecall"
    },
    {
      "name":"FilterManager",
      "class":"Manager",
      "open":1,
      "过滤":"串行执行过滤",
      "functors":"SimpleFilter,BizFilter"
    },
    {
      "name":"SortManager",
      "class":"Manager",
      "open":1,
      "排序":"",
      "functors":"PredictSort,SimpleSort"
    },
    {
      "name":"AdjustManager",
      "class":"Manager",
      "open":1,
      "调权":"",
      "functors":"SimpleAdjust,ChnAdjust"
    },
    {
      "name":"ResortManager",
      "class":"Manager",
      "open":1,
      "重排":"",
      "functors":"SimpleResort"
    },
    {
      "name":"PositionManager",
      "class":"Manager",
      "open":1,
      "占位":"串行执行，前面算子优先占位，后边算子继续占剩余的位",
      "functors":"StrongPosition,ChnPosition,SimplePosition"
    },
    {
      "name":"EndManager",
      "class":"Manager",
      "open":1,
      "结束":"返回请求结果，日志落盘",
      "functors":"EventToResponse,LogDump"
    },
    {
      "name":"SessionRecall",
      "class":"SessionRecall",
      "open":1,
      "exceptNum":100,
      "规则表达示":"种子过滤规则",
      "express":"actDay<30 || version>8 || \"man\".equals(sex)",
      "seedExpress":"dr>10 && views>1 || likes>0 || pay>1"
    },
    {
      "name":"CoverFilter",
      "class":"SimpleFilter",
      "open":1,
      "规则表达示":"圏用户",
      "express":"highActive == true && \"ios\".equals(os) && offset<100"
    },
    {
      "name":"ChnAdjust",
      "open":1,
      "class":"ChnAdjust",
      "config": {
        "chn":"SimpleRedis",
        "weight":5.5,
        "规则表达示":"圏用户",
        "express":"lowActive == true"
      }
    },
    {
      "name":"ChnPosition",
      "class":"ChnPosition",
      "open":1,
      "config": {
        "chnPos":"SessionRecall:4,5,6"
      }
    }
  ]
}